    <script type="text/javascript">
      $('#map_canvas').gmap().bind('init', function() {
        // This URL won't work on your localhost, so you need to change it
        // see http://en.wikipedia.org/wiki/Same_origin_policy
        $.ajax({
          url: '<%= @url %>',
          dataType: 'jsonp',
          data: "q=asset:355131040628343",
          success: function(data) {
            $.each( data, function(i, track) {
              console.log(track);

              if (track.location) {
                $('#map_canvas').gmap('addMarker', {
                  'position': new google.maps.LatLng(track.location[1], track.location[0]),
                  'bounds': true
                }).click(function() {
                  var str = "";
                  var fields = track.fields;

                  $.each( track.fields, function(key, value) {
                    str = str + key + ": "
                    if ( value.converted_value !== undefined ) {
                      str = str + value.converted_value
                    } else if ( value.decoded_value !== undefined ) {
                      str = str + value.decoded_value
                    } else {
                      str = str + value.b64_value + ' (base64)'
                    }

                    str = str + "<br/>"
                  })
                  if (str != "" ) {
                    $('#map_canvas').gmap('openInfoWindow', { 'content': "" + str }, this);
                  } else {
                    $('#map_canvas').gmap('closeInfoWindow');
                  }
                });
              }
            });
          }
        });
      });
    </script>
  </body>
</html>
